block config
	- var pathImg = ['images/thumb-img/', 'images/ekonografik/']

doctype html
html.no-js(lang="id")
	head
		meta(charset="utf-8")
		meta(name="description" content="")
		meta(name="viewport" content="width=device-width, initial-scale=1")
		title new_katadata
		link(rel="apple-touch-icon" href="apple-touch-icon.png")
		link(rel='stylesheet', href='https://cloud.typography.com/6761036/6309792/css/fonts.css')
		// Place favicon.ico in the root directory
		// build:css styles/vendor.css
		// bower:css
		link(rel='stylesheet', href='/bower_components/slick-carousel/slick/slick.css')
		link(rel='stylesheet', href='/bower_components/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css')
		// endbower
		// endbuild
		// build:css styles/main.css
		link(rel="stylesheet" href="styles/main.css")
		// endbuild
		// build:js scripts/vendor/modernizr.js
		script(src="/bower_components/modernizr/modernizr.js")
		// endbuild
	body
		//if IE
			p.browserupgrade
				| You are using an 
				strong outdated
				|  browser. Please 
				a(href="http://browsehappy.com/") upgrade your browser
				|  to improve your experience.
				
		div(id="mainWrapper" class="wrapper")
			#mp-pusher.mp-pusher
				include ../includes/kd-menu-nav.pug
				a(href="#" class="goUp")
					i(class="fa fa-chevron-up")
				header
					include ../includes/header.pug
				main
					.contentWrapper
						.member-area
							block member-caption
							block member-content
						include ../includes/footer.pug
		
		// build:js scripts/vendor.js
		// bower:js
		script(src='/bower_components/jquery/dist/jquery.js')
		script(src='/bower_components/modernizr/modernizr.js')
		script(src='/bower_components/slick-carousel/slick/slick.js')
		script(src='/bower_components/matchHeight/dist/jquery.matchHeight.js')
		script(src='/bower_components/moment/moment.js')
		script(src='/bower_components/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js')
